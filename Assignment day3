//Assignment day3
//To print pattern
/*  * * * *
     * * * *
      * * * *
       * * * *  */
       
       
  #include<iostream>
  using namespace std;
  int main()
  {
     for(int i=0;i<5;i++)
     {
        for(int j=0;j<5;j++)
        {
          if(i<=j)
          {
             cout<<"* ";
           }
           else
           {
             cout<<" ";
           }
         }
      
      for(int k=0;k<=5;k++)
      {
         if(i>=k)
           cout<<"* ";
      }
       cout<<endl;
    }
  }
  Output
* * * * * * 
 * * * * * * 
  * * * * * * 
   * * * * * * 
    * * * * * * 
